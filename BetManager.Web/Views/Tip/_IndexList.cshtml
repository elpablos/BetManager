@model IEnumerable<BetManager.Web.Controllers.Tips.List.TipListItemViewModel>


<div class="row">
  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <table id="example" class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th data-priority="1">
            @Html.DisplayNameFor(model => model.DisplayName)
          </th>
          <th data-priority="2">
            @Html.DisplayNameFor(model => model.DateStart)
          </th>
          <th>
            @Html.DisplayNameFor(model => model.PredictTip)
          </th>
          <th data-priority="1">
            @Html.DisplayNameFor(model => model.Home)
          </th>
          <th  data-priority="1">
            @Html.DisplayNameFor(model => model.Draw)
          </th>
          <th  data-priority="1">
            @Html.DisplayNameFor(model => model.Away)
          </th>
          <th  data-priority="1">
            @Html.DisplayNameFor(model => model.Odd)
          </th>
          <th>
            @Html.DisplayNameFor(model => model.WinnerCode)
          </th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        @foreach (var item in Model)
        {
            <tr>
              <td>
                @Html.DisplayFor(modelItem => item.DisplayName)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.DateStart)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.PredictTip)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.Home)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.Draw)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.Away)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.Odd)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.WinnerCode)
              </td>
              <td>
                @Html.ActionLink("Detail", "Detail", new { id = item.ID }, htmlAttributes: new { @class= "btn btn-primary" })
              </td>
            </tr>
        }
      </tbody>
    </table>
  </div>
</div>
