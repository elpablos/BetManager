@model IEnumerable<BetManager.Web.Controllers.Tips.Detail.DetailTipPoissonViewModel>

<table id="example" class="table table-striped table-bordered table-hover table-responsive">
  <thead>
    <tr>
      <td>
        @Html.DisplayNameFor(model => model.DisplayName)
      </td>
      <td class="text-center">
        @Html.DisplayNameFor(model => model.GoalZero)
      </td>
      <td class="text-center">
        @Html.DisplayNameFor(model => model.GoalOne)
      </td>
      <td class="text-center">
        @Html.DisplayNameFor(model => model.GoalTwo)
      </td>
      <td class="text-center">
        @Html.DisplayNameFor(model => model.GoalThree)
      </td>
      <td class="text-center">
        @Html.DisplayNameFor(model => model.GoalFour)
      </td>
      <td class="text-center">
        @Html.DisplayNameFor(model => model.GoalFive)
      </td>
      <td class="text-center">
        @Html.DisplayNameFor(model => model.Tip)
      </td>
      <td class="text-center">
        @Html.DisplayNameFor(model => model.Score)
      </td>
    </tr>
  </thead>
  <tbody>
    @foreach (var item in Model)
    {
        <tr>
          <td>
            @Html.DisplayFor(modelItem => item.DisplayName)
          </td>
          <td class="text-right">
            @Html.DisplayFor(modelItem => item.GoalZero) %
          </td>
          <td class="text-right">
            @Html.DisplayFor(modelItem => item.GoalOne) %
          </td>
          <td class="text-right">
            @Html.DisplayFor(modelItem => item.GoalTwo) %
          </td>
          <td class="text-right">
            @Html.DisplayFor(modelItem => item.GoalThree) %
          </td>
          <td class="text-right">
            @Html.DisplayFor(modelItem => item.GoalFour) %
          </td>
          <td class="text-right">
            @Html.DisplayFor(modelItem => item.GoalFive) %
          </td>
          <td class="text-center info">
            <strong>@Html.DisplayFor(modelItem => item.Tip)</strong>
          </td>
          <td class="text-center">
            @Html.DisplayFor(modelItem => item.Score)
          </td>
        </tr>
    }
  </tbody>
</table>
