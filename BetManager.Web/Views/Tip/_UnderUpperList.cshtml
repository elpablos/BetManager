@model IEnumerable<BetManager.Web.Controllers.Tips.UnderUpper.TipUnderUpperItemViewModel>


<div class="row">
  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <table id="example" class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th class="all">
            @Html.DisplayNameFor(model => model.DisplayName)
          </th>
          <th class="min-tablet-p">
            @Html.DisplayNameFor(model => model.DateStart)
          </th>
          <th class="all">
            1.5 %
            @*@Html.DisplayNameFor(model => model.OneAndHalfMinus)*@
          </th>
          <th class="never">
            @Html.DisplayNameFor(model => model.OneAndHalfPlus)
          </th>
          <th class="all">
            2.5 %
            @*@Html.DisplayNameFor(model => model.TwoAndHalfMinus)*@
          </th>
          <th class="never">
            @Html.DisplayNameFor(model => model.TwoAndHalfPlus)
          </th>
          <th class="never">
            @Html.DisplayNameFor(model => model.OneAndHalfMinusOdd)
          </th>
          <th class="never">
            @Html.DisplayNameFor(model => model.OneAndHalfPlusOdd)
          </th>
          <th class="never">
            @Html.DisplayNameFor(model => model.TwoAndHalfMinusOdd)
          </th>
          <th class="never">
            @Html.DisplayNameFor(model => model.TwoAndHalfPlusOdd)
          </th>
          <th class="min-tablet-p">
            @Html.DisplayNameFor(model => model.Category)
          </th>
          <th class="min-tablet-p">
            @Html.DisplayNameFor(model => model.Goals)
          </th>
          <th class="never">
            @Html.DisplayNameFor(model => model.WinnerCode)
          </th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        @foreach (var item in Model)
        {
            <tr>
              <td>
                @Html.DisplayFor(modelItem => item.DisplayName)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.DateStart)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.OneAndHalfMinus)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.OneAndHalfPlus)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.TwoAndHalfMinus)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.TwoAndHalfPlus)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.OneAndHalfMinusOdd)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.OneAndHalfPlusOdd)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.TwoAndHalfMinusOdd)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.TwoAndHalfPlusOdd)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.Category)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.Goals)
              </td>
              <td>
                @Html.DisplayFor(modelItem => item.WinnerCode)
              </td>
              <td>
                @Html.ActionLink("Detail", "Detail", new { id = item.ID }, htmlAttributes: new { @class = "btn btn-primary" })
              </td>
            </tr>
        }
      </tbody>
    </table>
  </div>
</div>
